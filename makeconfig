# If you set to yes a pure Lua interpreter will be built.
# It is meant to be compatible with standard Lua and short function syntax
# will be disabled.
LUA_BUILD = no

DEBUG = no

USE_READLINE = yes

PREFIX= /usr
DESTDIR=
